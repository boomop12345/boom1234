#include <iostream>
using namespace std;
int main()
{
    int a[10],i,j,k,t;
    cout<<"enter 10 elements:"<<endl;
    for(i=0;i<10;i++)
    {
        cin>>a[i];
    }
    cout<<"Entered elements are: \n";
    for(i=0;i<10;i++)
    {
        cout<<a[i]<<endl;
    }
    for (k=1;k<10;k++)
    {
        for (j=0;j<(10-k);j++)
        {
            if (a[j]>a[j+1])
            {
                t=a[j];
                a[j]=a[j+1];
                a[j+1]=t;
            }
        }
    }
    cout<<"the aorted elements are : \n";
    for (j=0;j<10;j++)
    {
        cout<<a[j]<<endl;
    }
    return 0;
    
}
